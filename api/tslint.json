{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": false,
    "no-console": false,
    "member-ordering": [
      true,
      {
        "order": "fields-first"
      }
    ],
    "ordered-imports": true,
    "object-literal-sort-keys": true
  },
  "rulesDirectory": []
}