{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "lib": [
      "esnext"
    ],
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "outDir": "./tsc-out"
  },
  "exclude": [
    "node_modules",
    "tsc-out"
  ]
}